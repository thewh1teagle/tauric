{
    "productName": "Tauri App",
    "version": "0.1.0",
    "identifier": "com.tauri.dev",
    "build": {
      "frontendDist": "dist",
      "devUrl": "http://google.com",
      "beforeDevCommand": "npm run dev",
      "beforeBuildCommand": "npm run build"
    },
    "app": {
      "withGlobalTauri": true,
      "security": {
        "csp": null
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
  